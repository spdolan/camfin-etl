{
  "name": "camfin-etl",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/spdolan/camfin-etl.git",
  "author": "sean dolan <snpdolan@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:test": "cdk --app 'npx ts-node --prefer-ts-exts test/bin/index.ts'",
    "cdk:stage": "cdk --app 'npx ts-node --prefer-ts-exts staging/bin/index.ts'",
    "cdk:prod": "cdk --app 'npx ts-node --prefer-ts-exts prod/bin/index.ts'",
    "test:list": "yarn cdk:test list",
    "test:synth": "yarn cdk:test synth",
    "test:db": "yarn cdk:test deploy TestDatabase",
    "stage:list": "yarn cdk:stage list",
    "stage:synth": "yarn cdk:stage synth",
    "stage:db": "yarn cdk:stage deploy StagingDatabase",
    "prod:list": "yarn cdk:prod list",
    "prod:synth": "yarn cdk:prod synth",
    "prod:db": "yarn cdk:prod deploy ProdDatabase"
  },
  "devDependencies": {
    "@aws-cdk/assertions": "1.152.0",
    "@types/jest": "*",
    "@types/node": "^12",
    "@types/unzipper": "^0.10.5",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "esbuild": "^0.14.9",
    "eslint": "^8",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^27.4.5",
    "jest-junit": "^13",
    "json-schema": "^0.4.0",
    "npm-check-updates": "^12",
    "projen": "^0.47.4",
    "ts-jest": "^27.1.2",
    "ts-node": "^9",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@aws-cdk/aws-ec2": "^1.152.0",
    "@aws-cdk/aws-events-targets": "^1.152.0",
    "@aws-cdk/aws-iam": "^1.152.0",
    "@aws-cdk/aws-rds": "^1.152.0",
    "@aws-cdk/aws-s3": "^1.152.0",
    "@aws-cdk/aws-secretsmanager": "^1.152.0",
    "@aws-cdk/aws-sqs": "^1.152.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.152.0",
    "@aws-cdk/core": "^1.152.0",
    "constructs": "^10.0.5",
    "fast-csv": "^4.3.6",
    "unzipper": "^0.10.11"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "text"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.dev.json"
      }
    }
  }
}
