    "aws-cdk": "^2.1.0",
    "esbuild": "^0.14.9",
    "eslint": "^8",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^27.4.5",
    "jest-junit": "^13",
    "json-schema": "^0.4.0",
    "npm-check-updates": "^12",
